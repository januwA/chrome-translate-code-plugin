{
  "manifest_version": 2,
  "name": "Translate code",
  "version": "0.1.2",
  "description": "避免翻译页面中的代码",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://*.medium.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    },
    {
      "matches": [
        "*://*.medium.com/*"
      ],
      "js": [
        "js/medium-content.js"
      ]
    },
    {
      "matches": [
        "*://gist.github.com/*"
      ],
      "js": [
        "js/gist-github-content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}